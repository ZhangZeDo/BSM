A,KAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,IAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,MAAA;AACA,KAVA;AAWA,IAAA,WAXA,uBAWA,MAXA,EAWA;AACA,UAAA,MAAA,IAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA,KAjBA;AAkBA,IAAA,SAlBA,uBAkBA;AAAA;;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,qBAAA,EAAA;AACA,QAAA,WAAA,EAAA,KAAA,SAAA,CAAA,WADA;AAEA,QAAA,MAAA,EAAA,KAAA,SAAA,CAAA,MAFA;AAGA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA,QAHA;AAIA,QAAA,IAAA,EAAA,KAAA,SAAA,CAAA,IAJA;AAKA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA;AALA,OAAA,EAMA,IANA,CAMA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,OAXA;AAYA,KA/BA;AAgCA,IAAA,gBAhCA,4BAgCA,GAhCA,EAgCA,OAhCA,EAgCA;AAAA;;AACA,WAAA,QAAA,CAAA,SAAA,OAAA,GAAA,YAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,wBAAA,EAAA;AACA,UAAA,EAAA,EAAA,GAAA,CAAA,EADA;AAEA,UAAA,MAAA,EAAA,GAAA,CAAA;AAFA,SAAA,EAGA,IAHA,CAGA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,SAAA;AACA;AACA,SARA;AASA,OAdA,EAcA,KAdA,CAcA,YAAA,CAEA,CAhBA;AAiBA,KAlDA;AAmDA,IAAA,UAnDA,wBAmDA;AAAA;;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QADA;AAEA,QAAA,WAAA,EAAA,KAAA,QAAA,CAAA,WAFA;AAGA,QAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAHA;AAIA,QAAA,YAAA,EAAA,KAAA,QAAA,CAAA;AAJA,OAAA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,gCAAA;AACA;AACA,OATA;AAUA,KA9DA;AA+DA,IAAA,gBA/DA,4BA+DA,GA/DA,EA+DA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA,KAlEA;AAmEA,IAAA,mBAnEA,+BAmEA,GAnEA,EAmEA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA;AAtEA;AAlBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"card-header\" style=\"margin-top: 20px;margin-left: 100px\">\r\n            <h3>用户管理</h3>\r\n        </div>\r\n        <el-card style=\"margin:20px 100px;\">\r\n            <div slot=\"header\" style=\"height: 30px\">\r\n                <div style=\"float: left;margin-right: 10px;\">\r\n                    <label style=\"\">账号或名称:</label>\r\n                </div>\r\n                <div style=\"float: left;margin-right: 10px\">\r\n                    <el-input size=\"small\" placeholder=\"搜索账号或名称\" v-model=\"queryForm.searchInput\" clearable @change=\"queryData()\">\r\n                        <i slot=\"prefix\" class=\"el-input__icon el-icon-search\"></i>\r\n                    </el-input>\r\n                </div>\r\n                <div style=\"float: left;margin-right: 10px;padding-left: 20px\">\r\n                    <label style=\"\">用户状态:</label>\r\n                </div>\r\n                <div style=\"float: left;margin-right: 10px\">\r\n                    <el-select size=\"small\" v-model=\"queryForm.status\" clearable placeholder=\"请选择用户状态\" @change=\"queryData()\">\r\n                        <el-option value=\"1\" label=\"有效\">有效</el-option>\r\n                        <el-option value=\"0\" label=\"无效\">无效</el-option>\r\n                    </el-select>\r\n                </div>\r\n            </div>\r\n            <div>\r\n                <el-table :data=\"userList\" style=\"width: 100%\" stripe>\r\n                    <el-table-column prop=\"userAccount\" label=\"管理员账号\" style=\"width: 15%\"></el-table-column>\r\n                    <el-table-column prop=\"userName\" label=\"管理员名称\" style=\"width: 15%\"></el-table-column>\r\n                    <el-table-column prop=\"userMail\" label=\"邮箱地址\" style=\"width: 15%\"></el-table-column>\r\n                    <el-table-column prop=\"userPhone\" label=\"联系方式\" style=\"width: 15%\"></el-table-column>\r\n                    <el-table-column  label=\"创建时间\" style=\"width: 15%\" >\r\n                        <template slot-scope=\"scope\">\r\n                            {{parseTime(scope.row.createDatetime) }}\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"状态\" style=\"width: 15%\">\r\n                        <template slot-scope=\"scope\">\r\n                            {{parseStatus(scope.row.status) }}\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"操作\" style=\"width: 10%\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button v-if=\"scope.row.status == 0\" @click=\"changeUserStatus(scope.row,'启用')\"  type=\"primary\" size=\"mini\">启用</el-button>\r\n                            <el-button v-else @click=\"changeUserStatus(scope.row,'禁用')\"  type=\"danger\" size=\"mini\">禁用</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <div style=\"padding-top: 20px\">\r\n                <div class=\"block\">\r\n                    <el-pagination\r\n                            @size-change=\"handleSizeChange\"\r\n                            @current-change=\"handleCurrentChange\"\r\n                            :current-page=\"queryForm.page\"\r\n                            :page-size=\"queryForm.pageSize\"\r\n                            :page-sizes=\"[5,10,15,20]\"\r\n                            layout=\"total, sizes,prev, pager, next, jumper\"\r\n                            :total=\"total\">\r\n                    </el-pagination>\r\n                </div>\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"userManage\",\r\n        data(){\r\n            return {\r\n                queryForm:{\r\n                    searchInput:'',\r\n                    status:'',\r\n                    pageSize:5,\r\n                    page:1,\r\n                    roleType:'ordinary',\r\n                },\r\n                userList:[],\r\n                total:0,\r\n            }\r\n        },\r\n        created() {\r\n            this.queryData();\r\n        },\r\n        methods:{\r\n            parseTime(dateObj) {\r\n                var date = new Date(dateObj);\r\n                var year = date.getFullYear();\r\n                var month = (\"0\" + (date.getMonth() + 1)).slice(-2);\r\n                var day = (\"0\" + date.getDate()).slice(-2);\r\n                var hour = (\"0\" + date.getHours()).slice(-2);\r\n                var min = (\"0\" + date.getMinutes()).slice(-2);\r\n                var second = (\"0\" + date.getSeconds()).slice(-2);\r\n                return year + \"-\" + month + \"-\" + day + \" \" + hour + \":\" + min + \":\" + second;\r\n            },\r\n            parseStatus(status){\r\n                if (status == 0){\r\n                    return \"无效\";\r\n                }else{\r\n                    return \"有效\"\r\n                }\r\n            },\r\n            queryData(){\r\n                this.$axios.post('/user/queryUserList',{\r\n                    searchInput:this.queryForm.searchInput,\r\n                    status:this.queryForm.status,\r\n                    pageSize:this.queryForm.pageSize,\r\n                    page:this.queryForm.page,\r\n                    roleType:this.queryForm.roleType\r\n                }).then(resp=>{\r\n                    if (resp.code == 200) {\r\n                        this.userList = resp.data.items;\r\n                        this.total = resp.data.total;\r\n                    }\r\n                });\r\n            },\r\n            changeUserStatus(row,operate){\r\n                this.$confirm('此操作将'+operate+'该用户, 是否继续?', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => {\r\n                    this.$axios.post('/user/changeUserStatus',{\r\n                        id:row.id,\r\n                        status: row.status\r\n                    }).then(resp=>{\r\n                        if (resp.code == 200){\r\n                            this.$message.success(\"操作成功\");\r\n                            this.queryData();\r\n                        }\r\n                    })\r\n                }).catch(() => {\r\n\r\n                });\r\n            },\r\n            submitForm(){\r\n                this.$axios.post('/user/register',{\r\n                    userName:this.userForm.userName,\r\n                    userAccount:this.userForm.userAccount,\r\n                    roleType:this.userForm.roleType,\r\n                    userPassword:this.userForm.userPassword\r\n                }).then(resp=>{\r\n                    if (resp.code == 200) {\r\n                        this.$message.success(\"管理员创建成功,初始密码为000000,请联系管理员完善信息\");\r\n                    }\r\n                });\r\n            },\r\n            handleSizeChange(val) {\r\n                this.queryForm.pageSize = val;\r\n                this.queryData()\r\n            },\r\n            handleCurrentChange(val) {\r\n                this.queryForm.page = val;\r\n                this.queryData()\r\n            },\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/personManage"}]}