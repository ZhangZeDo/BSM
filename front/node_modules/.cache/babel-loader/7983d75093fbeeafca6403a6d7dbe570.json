UAAA,OAAA,EAAA,OAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,EAEA;AAAA,UAAA,GAAA,EAAA,CAAA;AAAA,UAAA,GAAA,EAAA,EAAA;AAAA,UAAA,OAAA,EAAA,UAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAFA;AAbA;AAVA,KAAA;AA6BA,GAhCA;AAiCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAAA;;AACA,UAAA,KAAA,QAAA,CAAA,YAAA,IAAA,KAAA,cAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA,OAFA,MAEA,IAAA,CAAA,KAAA,QAAA,CAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,UAAA,WAAA,EAAA,KAAA,QAAA,CAAA,WADA;AAEA,UAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAFA;AAGA,UAAA,YAAA,EAAA,KAAA,QAAA,CAAA,YAHA;AAIA,UAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAJA;AAKA,UAAA,SAAA,EAAA,KAAA,QAAA,CAAA,SALA;AAMA,UAAA,QAAA,EAAA,KAAA,QAAA,CAAA;AANA,SAAA,EAOA,IAPA,CAOA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA;AACA,SAZA;AAaA;AACA;AArBA;AAjCA,CAAA","sourcesContent":["<template>\r\n    <div class=\"register\">\r\n        <div style=\"display: flex;justify-content: center;margin-top: 150px\">\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" auto-complete=\"on\" label-position=\"left\">\r\n                <h3 class=\"title\" align=\"center\">用户注册</h3>\r\n                <el-card style=\"width: 450px;margin-top: 20px\">\r\n                    <el-form-item prop=\"userAccount\">\r\n                        <el-input style=\"margin-top: 20px\" v-model=\"ruleForm.userAccount\" placeholder=\"学号或工号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item prop=\"userName\">\r\n                        <el-input style=\"margin-top: 10px\" v-model=\"ruleForm.userName\" placeholder=\"请输入昵称\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item prop=\"userPassword\">\r\n                        <el-input style=\"margin-top: 10px\" v-model=\"ruleForm.userPassword\" placeholder=\"请输入密码\" show-password></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item prop=\"reUserPassWord\">\r\n                        <el-input style=\"margin-top: 10px\" v-model=\"reUserPassWord\" placeholder=\"请确认你的密码\" show-password></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item prop=\"userMail\">\r\n                        <el-input style=\"margin-top: 10px\" v-model=\"ruleForm.userMail\" placeholder=\"请输入邮箱\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item prop=\"userPhone\">\r\n                        <el-input style=\"margin-top: 20px\" v-model=\"ruleForm.userPhone\" placeholder=\"请输入手机号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-button style=\"margin-top: 20px;width: 100%\" type=\"primary\" @click=\"doRegister\">注册</el-button>\r\n                </el-card>\r\n            </el-form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"register\",\r\n        data() {\r\n            return{\r\n                ruleForm:{\r\n                    userAccount:'',\r\n                    userName:'',\r\n                    userPassword:'',\r\n                    userMail:'',\r\n                    userPhone:'',\r\n                    roleType:'ordinary',\r\n                },\r\n                reUserPassWord:'',\r\n                rules:{\r\n                    userAccount: [\r\n                        { required: true, message: '请输入账号', trigger: 'blur' },\r\n                        { min: 3, max: 20, message: '长度在 3 到 20 个字符', trigger: 'blur' }\r\n                    ],\r\n                    userName: [\r\n                        { required: true, message: '请输入名称', trigger: 'blur' },\r\n                        { min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur' }\r\n                    ],\r\n                    userPassword: [\r\n                        { required: true, message: '请输入密码', trigger: 'blur' },\r\n                        { min: 6, max: 10, message: '长度在 6 到 10 个字符', trigger: 'blur' }\r\n                    ],\r\n                    userMail:[\r\n                        { required: true, message: '请输入邮箱', trigger: 'blur' },\r\n                        { min: 1, max: 30, message: '请输入正确的邮箱', trigger: 'blur' }\r\n                    ]\r\n                }\r\n            }\r\n        },\r\n        methods:{\r\n            doRegister(){\r\n                if (this.ruleForm.userPassword != this.reUserPassWord){\r\n                    this.$message.error(\"输入的2次密码不一致\")\r\n                }else if (!this.ruleForm.userMail){\r\n                    this.$message.error(\"请输入邮箱信息\")\r\n                }else{\r\n                    this.$axios.post('/user/register',{\r\n                        userAccount:this.ruleForm.userAccount,\r\n                        userName:this.ruleForm.userName,\r\n                        userPassword:this.ruleForm.userPassword,\r\n                        userMail:this.ruleForm.userMail,\r\n                        userPhone:this.ruleForm.userPhone,\r\n                        roleType:this.ruleForm.roleType,\r\n                    }).then(resp=>{\r\n                        if (resp.code == 200) {\r\n                            this.$message.success(\"注册成功\");\r\n                            this.$router.push({name: 'login'});\r\n                        }\r\n                    });\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .register{\r\n        background-image: url(\"../static/img/background.png\");\r\n        background-size: cover;\r\n        height: 650px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}